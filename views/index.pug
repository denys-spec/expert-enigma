html
  head
    title= title
  body
    body
    form(action='/', method='POST')
      p
        | title: 
        input(type='text', name='notetitle', value='')
        | text: 
        input(type='text', name='notetext', value='')
      input(type='submit', value='Submit')

    h1= message
    ul
      each obj in notes
        li= obj.title
        p= obj.text